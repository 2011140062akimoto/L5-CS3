<%= form_tag '/sessions/login', method: :post do %>
  <%= label_tag :uid, 'ユーザーID' %>
  <%= text_field_tag :uid %>
  <%= label_tag :pass, 'パスワード' %>
  <%= password_field_tag :pass %>
  <%= submit_tag 'ログイン' %>
<% end %>

<p> <%= link_to 'ユーザー一覧', users_path%> </p>
<p> <%= link_to '新規ユーザー作成', sessions_signin_view_path%> </p>